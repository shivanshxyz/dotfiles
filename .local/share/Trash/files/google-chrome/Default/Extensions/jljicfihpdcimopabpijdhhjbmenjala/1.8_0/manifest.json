{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "js": [ "jquery.min.js", "contentScript.js" ],
      "matches": [ "https://mail.google.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "Unsubscribe all unwanted emails in bulk with a click. Take control of your inbox",
   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "48": "icons/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Unsubscribe Gmail emails in bulk - HelpNinja",
   "oauth2": {
      "client_id": "44433400230-505bk22dig8k630gt7fncl2ndbvmso8q.apps.googleusercontent.com",
      "scopes": [ "https://www.googleapis.com/auth/gmail.settings.basic" ]
   },
   "permissions": [ "identity", "identity.email", "storage", "tabs", "https://mail.google.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.8",
   "web_accessible_resources": [ "include.js" ]
}
