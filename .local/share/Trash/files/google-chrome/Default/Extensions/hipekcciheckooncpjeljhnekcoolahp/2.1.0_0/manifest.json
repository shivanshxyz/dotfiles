{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_title": "Tabliss"
   },
   "chrome_url_overrides": {
      "newtab": "index.html"
   },
   "content_security_policy": "script-src 'self' https://www.google.com https://api.bing.com; object-src 'self'",
   "description": "A beautiful New Tab page with many customisable backgrounds and widgets that does not require any permissions.",
   "icons": {
      "128": "icons/128.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "96": "icons/96.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Tabliss - A Beautiful New Tab",
   "permissions": [ "storage" ],
   "short_name": "Tabliss",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.1.0"
}
